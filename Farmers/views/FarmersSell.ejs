<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">


    <!-- Bootstrap Core CSS -->
    <link href="/../vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="/../vendor/metisMenu/metisMenu.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="/../dist/css/sb-admin-2.css" rel="stylesheet">

    <!-- Morris Charts CSS -->
    <link href="/../vendor/morrisjs/morris.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="/../vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <script src="/../vendor/jquery/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/../vendor/bootstrap/js/bootstrap.min.js"></script>

   <script>


   </script>

</head>

<body>
    <div id="wrapper">

       

        <div id="page-wrapper">

            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Sellers Market
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <form role="form" method="post" action="/sell">
                                      

                                        <ul class="nav">
                                            <li>
                                              <div class="form-group">
                                                  <label>Farmers Market Ethererum Account Address </label>
                                                  <input class="form-control" id="FMID" name="FMID" placeholder="Farmers Market Ethereum Address">
                                              </div>
                                            <li>
											<li>
                                              <div class="form-group">
                                                  <label>Farmers Ethererum Account Address </label>
                                                  <input class="form-control" id="farmersId" name="farmersId" placeholder="Farmers Ethereum Address">
                                              </div>
                                            <li>
                                            <li>
                                                <div class="form-group">
                                                    <label>Weight of Crop</label>
                                                    <input class="form-control" id="weight" name="weight" placeholder="Weight (in Pounds)">
                                                </div>
                                            </li>
                                            
                                                <LI>
                                                        <button type="submit" id="submitData" class="btn btn-default">Submit</button>
                                                        <button type="reset" id="resetData" class="btn btn-default">Reset</button>
                                                </LI>
                                        </ul>

                                    </form>
                                </div>
                            </div>
                            <!-- /.row (nested) -->
                        </div>

                        <div class="panel-body">
                            <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                                <thead>
                                    <tr>
                                        <th>Transection Hash</th>
                                        <th>Farmer Address</th>
                                        <th>Farmers Market Address</th>
                                        <th>Weight of Goods</th>
                                        <th>Balance of Farmers</th>
                                        <th>Balance of Farmers Market</th>
                                    </tr>
                                </thead>
                                <tbody>
                                
                                    <% if(data.length){ 
                                        for(var i = 0;i < data.length;i++) { %>
                                      <tr>
                                        <td><%=data[i].TX_HASH%></td>
                                        <td><%=data[i].FETH_Address%></td>
                                        <td><%=data[i].FMETH_Address%></td>
                                        <td><%=data[i].weight%></td>
                                        <td><%=data[i].farmerbal%></td>
                                        <td><%=data[i].farmermbal%></td>
                                      </tr>
                                      <% }
                                      }else{ %>
                                      <tr>
                                        <td colspan="2">No user</td>
                                      </tr>
                                    <% } %>
            
                                </tbody>
                            </table>
            
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

    </div>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="/../vendor/metisMenu/metisMenu.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="/../dist/js/sb-admin-2.js"></script>

  <!--  <script>
     var studentId = document.getElementById("studentId");

      $("#addressList").change(function(){
        studentId.value = $("#addressList option:selected").val();
       }); 
    </script> -->
</body>

</html>

